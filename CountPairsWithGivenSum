#include <iostream>
using namespace std;
int main() { 
    int i, j, temp;
 int arr[]={ 1, 5, 7, -1};
 int sum=6;
 int count=0;
 int n=sizeof(arr)/sizeof(arr[0]);
 
 for (i=0;i<n;i++)
 {
     for(j=i+1;j<n;j++)
     {
     if(arr[i] + arr[j] ==sum)
         count++;
     }
}
cout<<"No. of Pairs Found is: "<<count<<endl;
return 0;
}
